@techreport{martinez_brechas_2018,
	address = {CDMX, Mexico},
	title = {{BRECHAS} {DE} {AGUA} {Y} {SANEAMIENTO} {EN} {MÉXICO}: {Resultados} recientes a partir del análisis de la {Encuesta} {Nacional} de los {Hogares} (forthcoming)},
	abstract = {A pesar de los grandes esfuerzos realizados en México, la Encuesta Nacional de los Hogares publicada en julio de 2018 por el Inegi, revela que  
únicamente el 53\% de la población habita en viviendas con acceso integral al ciclo de agua y saneamiento, es decir, viviendas que reciben agua diariamente, cuentan con excusado de uso exclusivo y están conectadas a una red de alcantarillado o drenaje.  El contraste es impresionante al interior del país, ya que el indicador alcanza un valor del 94\% en las zonas urbanas de Nuevo León, mientras que en las zonas rurales de Guerrero es de apenas el 7\%.  Lo anterior muestra que, aunque se han logrado avances importantes en llevar infraestructura de agua potable y de alcantarillado a la población, ahora es necesario mejorar la gestión para lograr que la población reciba los servicios de forma adecuada y ejerza su derecho al agua potable y al saneamiento.  Es necesario fortalecer a los organismos que prestan los servicios de agua y saneamiento para lograr prestar los servicios al nivel que aspiran los objetivos de desarrollo sostenible.},
	author = {Martinez, Ricardo},
	month = dec,
	year = {2018},
	file = {NotaTecnicaAguaySaneamientoENH_v6.docx:/Users/luisamimmi/Documents/02_Reference/Zotero/storage/GYBQ8D7C/NotaTecnicaAguaySaneamientoENH_v6.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document}
}

@techreport{riquelme_organismos_2018,
	address = {CDMX, Mexico},
	title = {{LOS} {ORGANISMOS} {OPERADORES} {DE} {AGUA} {Y} {SANEAMIENTO} {EN} {MÉXICO}},
	abstract = {En 1983 se reforma el artículo 115 de la Constitución Política de México y se inicia un proceso de transferencia de la responsabilidad de prestar los servicios de agua potable y saneamiento de la federación hacia los gobiernos municipales.  A partir de entonces, el Gobierno Federal fomenta en los municipios la creación de organismos especializados, con finanzas propias y autonomía de gestión.  Se crearon así diversas modalidades de prestación del servicio, desde los organismos públicos descentralizados del municipio o del estado, hasta concesiones a empresas privadas o asociaciones público-privadas. Este documento intenta mostrar una taxonomía completa de los diferentes tipos de organismos que existen en el país, con el fin de diseñar estrategias apropiadas para cada tipo de prestadores de los servicios.},
	author = {Riquelme, Rodrigo and Martinez, Ricardo},
	month = dec,
	year = {2018},
	file = {NotaTecnicaOrganismosOperadoresAyS_v3.docx:/Users/luisamimmi/Documents/02_Reference/Zotero/storage/VLRHLLQ9/NotaTecnicaOrganismosOperadoresAyS_v3.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document}
}

